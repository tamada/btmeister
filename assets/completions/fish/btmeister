complete -c btmeister -s D -l definition -d 'Specify the definition of the build tools.' -r -F
complete -c btmeister -l append-defs -d 'Specify the additional definitions of the build tools.' -r -F
complete -c btmeister -s i -l ignore-type -d 'Specify the ignore type.' -r -f -a "default\t'[IgnoreType::Ignore], [IgnoreType::GitIgnore], [IgnoreType::GitGlobal], and [IgnoreType::GitExclude]. All of the ignore types are enabled'
hidden\t'ignore hidden file'
ignore\t'ignore respecting `.ignore` file'
git-ignore\t'ignore respecting `.gitignore` file'
git-global\t'ignore respecting global git ignore file'
git-exclude\t'ignore respecting `.git/info/exclude` file'"
complete -c btmeister -s e -l excludes -d 'Specify the filters of excluding files or directories.' -r
complete -c btmeister -s f -l format -d 'Specify the output format' -r -f -a "csv\t''
default\t''
json\t''
markdown\t''
xml\t''
yaml\t''"
complete -c btmeister -s l -l level -d 'Specify the log level.' -r -f -a "error\t''
warn\t''
info\t''
debug\t''
trace\t''"
complete -c btmeister -l completion-out-dir -d 'Output directory of completion files' -r -F
complete -c btmeister -s L -l list-defs -d 'Print the build tools\' definition list'
complete -c btmeister -l generate-completion-files -d 'Generate completion files'
complete -c btmeister -s h -l help -d 'Print help (see more with \'--help\')'
complete -c btmeister -s V -l version -d 'Print version'
